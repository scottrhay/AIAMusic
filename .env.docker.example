# AIAMusic Docker Environment Variables
# Copy this file to .env and fill in your values

# Secret Keys (generate with: openssl rand -hex 32)
SECRET_KEY=your-secret-key-here-change-me
JWT_SECRET_KEY=your-jwt-secret-key-here-change-me

# Database Configuration (keep existing sunoapp_db to preserve data)
DB_NAME=sunoapp_db
DB_USER=sunoapp_user
DB_PASSWORD=your-database-password-from-setup

# Domain (for CORS and SSL)
DOMAIN=music.aiacopilot.com
